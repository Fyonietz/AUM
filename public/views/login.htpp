            <div class="input-block">
                <label for="Password">PASSWORD:</label>
                <input type="text" name="Password">
            </div>
        </div>
        <div class="container-dum container-dum-hover"><button onclick="login()">LOGIN</button></div>
    </div>
    <script src="../script.js"></script>
</body>
</html>